  <div id="page-wrapper" class="well">

    <header class="page-header">
      <form id="animal-form" class="form">
        <input name="name" type="text" class="form-control" placeholder="Name of animal">
        <input name="breed" type="text" class="form-control" placeholder="Breed">
        <input name="dob" type="text" class="form-control" placeholder="dd/mm/yyyy">
        <input name="gender" type="text" class="form-control" placeholder="Gender(M/F)">
        <input name="family" type="text" class="form-control" placeholder="Family">
        <input name="status" type="hidden" value="Adopt">
        <button type="submit" class="btn btn-default">Add animal</button>
      </form>
    </header>
    <ul class="list" id="animalTemplate-ul"></ul>
  </div>




